#!/bin/sh

set -e

#PKGOS-INCLUDE#

if [ "$1" = "configure" ] || [ "$1" = "reconfigure" ] ; then
	. /usr/share/debconf/confmodule
	. /usr/share/dbconfig-common/dpkg/postinst

	pkgos_register_endpoint_postinst octavia octavia load-balancer "Octavia Load Balancing Service" 9876 ""
	db_stop
fi

#DEBHELPER#

exit 0
